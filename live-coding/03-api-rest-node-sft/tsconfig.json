{
    "compilerOptions": {
        "baseUrl": "./",
        "paths": {
            "#src/*": [
                "./src/*"
            ]
        },
        "types": [
            "bun-types"
        ],
        "esModuleInterop": true,
        "skipLibCheck": true,
        "target": "ESNext",
        "verbatimModuleSyntax": true,
        "allowJs": true,
        "moduleDetection": "force",
        "moduleResolution": "NodeNext",
        "module": "NodeNext",
    },
}